import pandas as pd
import numpy as np

def read_data():
    df = pd.read_excel(r"CXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXx")
    df = df.drop(df.iloc[:, 25:],axis = 1)
    df.dropna(subset=['designation'])
    df.dropna()
    df = df.fillna("unknown")
    df_list = df.set_index('designation').T.to_dict("list")
    df_dict = df.set_index('designation').T.to_dict()
    df_dict_brand = df.set_index('supplier ').T.to_dict("list")
    return df_list, df_dict, df_dict_brand


data_list, data_dict, data_brand = read_data()
